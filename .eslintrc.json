{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "eol-last": "off",
      "arrow-parens": "off",
      "no-console": "off"
    },
    "ignorePatterns": [
      "dist/",
      "build/",
      "webpack/",
      "src/Scenes",
      "src/Objects",
      "src/Config",
      "src/Model.js"
    ]
  }